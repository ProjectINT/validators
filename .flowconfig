[include]

[ignore]
./node_modules/*
./dist/*

[untyped]
.*/node_modules/react-select/.*
.*/node_modules/react-stripe-elements/.*
.*/node_modules/react-transition-group/.*

[declarations]
<PROJECT_ROOT>/node_modules/graphql-relay/.*
<PROJECT_ROOT>/node_modules/react-relay-*
<PROJECT_ROOT>/node_modules/react-relay/.*
<PROJECT_ROOT>/node_modules/relay-runtime/.*
<PROJECT_ROOT>/node_modules/express-graphql/
<PROJECT_ROOT>/node_modules/graphql/.*
<PROJECT_ROOT>/node_modules/draft-js/.*
<PROJECT_ROOT>/__generated__/.*
<PROJECT_ROOT>/__graphql__/.*

[libs]

[options]
include_warnings=true
; This option lets you alias 'any' with a given string - useful for explaining why youâ€™re using 'any'
; Please, do not use "fix me" suppress types and comments
suppress_type=$FlowFixMe

[lints]
unnecessary-optional-chain=error
untyped-import=error
untyped-type-import=error

[strict]
unsafe-getters-setters
untyped-import
untyped-type-import
unclear-type

[version]
0.196.3
